        <ProgressIndicator layoutX="225.0" layoutY="82.0" progress="0.0" />
        <Button fx:id="generateVocabulary" onAction="#GenerateVocabulary" layoutX="54.0" layoutY="207.0" mnemonicParsing="false" text="Generar Vocabulario" />
      </children>
    </Pane>
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
